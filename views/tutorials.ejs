.filters-body .card footer {
	background: #fff;
	z-index: 10;
	padding: 20px;
}

.filters-body .card footer .item-tags {
	font-size: 11px;
	line-height: 1.4;
	color: #505050 !important;
}

.filters-body .card footer .item-social-share {
	margin: 0 auto 30px;
}

.filters-body .card footer .item-social-share .share_icon {
	height: 30px;
	width: 30px;
}

.filters-body .card footer .item-social-share .share_icon:before {
	font-size: 16px;
	line-height: 1;
	position: absolute;
	top: 21%;
	left: 25%;
	vertical-align: middle;
}

.card .card-back-wrap {
	transform: translateY(1000px);
	display: block;
	background-color: #d9e1e2;
	width: 100%;
	height: auto;
	padding: 15px 20px;
	position: absolute;
	transition: transform .5s;
	top: 0;
	right: 0;
	bottom: 105px;
	left: 0;
	font-size: 15px;
	line-height: 1.6;
	color: #222222;
}

.card .card-back-wrap .card-back-title {
	font-size: 11px;
	letter-spacing: 1.5px;
	color: #4e758b;
	margin: 0 auto 10px;
	text-transform: capitalize;
}

.card .card-back-wrap .card-back-content {
