	line-height: 1.6;
	color: #222222;
	margin: 0 auto 30px;
}

.card .card-back-wrap .card-back-btn {
	margin: 0 auto 30px;
	font-size: 14px;
	letter-spacing: 1.5px;
	color: #ffffff;
	text-transform: capitalize;
}

.card .card-back-wrap .card-back-foot {
	padding: 15px 0;
	position: relative;
	font-size: 11px;
	line-height: 1.4;
	color: #505050;
	text-transform: capitalize !important;
}

.card .card-back-wrap .card-back-foot:before {
	content: '';
	display: block;
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	width: 90px;
	border-top: 1px solid #505050;
}

.card:hover .card-back-wrap {
	transform: translateY(0px);
	height: 100%;
}

.card.live-event:hover .card-back-wrap {
	transform: translateY(175px);
}
